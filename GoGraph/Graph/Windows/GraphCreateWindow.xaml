        <TextBlock Text="Select graph type:" FontSize="18" Margin="20, 20, 0, 0" Width="160" Height="30" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <ListBox FontSize="18" Margin="180, 20, 0, 0" Width="200" Height="30" VerticalAlignment="Top" HorizontalAlignment="Left"/>
    </Grid>
</Window>
